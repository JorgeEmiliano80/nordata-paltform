
# Nueva Configuración - Databricks + GCP
VITE_APP_NAME=NORDATA.AI Platform
VITE_APP_VERSION=2.0.0

# Google Cloud Platform
VITE_GCP_PROJECT_ID=nordata-platform
VITE_GCP_REGION=us-central1
VITE_GCP_STORAGE_BUCKET=nordata-files
VITE_GCP_FUNCTIONS_URL=https://us-central1-nordata-platform.cloudfunctions.net
VITE_GCP_BIGQUERY_DATASET=nordata_analytics

# Databricks
VITE_DATABRICKS_WORKSPACE_URL=https://your-workspace.cloud.databricks.com
VITE_DATABRICKS_TOKEN=your-databricks-token
VITE_DATABRICKS_CLUSTER_ID=your-cluster-id

# Firebase (para autenticación)
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=nordata-platform.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=nordata-platform

# Desarrollo
VITE_DEV_MODE=true
VITE_API_TIMEOUT=30000
